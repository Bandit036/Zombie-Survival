import greenfoot.*;  // (World, Actor, GreenfootImage, Greenfoot and MouseInfo)

/**
 * This is where I can test to see if a variable is working correctly
 * 
 * @author Eric Faltermeier 
 * @version (a version number or a date)
 */
public class TestVariable extends Actor
{
    Zombie zombie;
    Bullet bullet;
    Human human;
    Reloading reloading;
    /**
     * Act - do whatever the TestVariable wants to do. This method is called whenever
     * the 'Act' or 'Run' button gets pressed in the environment.
     */
    public void act() 
    {
       setImage (new GreenfootImage("" + human.gunDelay, 30, Color.BLACK, Color.WHITE));
    }    
}
